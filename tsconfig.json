{
    "include": ["src/**/*.ts"],
    "compilerOptions": {
        "noEmit": true,
        "strict": true,
        "noImplicitAny": false,
        "noUncheckedIndexedAccess": true,
        "module": "NodeNext",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowImportingTsExtensions": true,
        "verbatimModuleSyntax": true,
	}
}
